#Calendars{
    flex-direction: row;   /* 横向排列 */
    flex-wrap: nowrap;     /* 禁止换行 */
    justify-content: space-between; /* 按钮均分间距 */
    padding: 0;
}

#Calendars Button {
    border-top-left-radius: 60px;
    border-top-right-radius: 60px;
    border-bottom-right-radius: 60px;
    border-bottom-left-radius: 60px;
    margin: 0;
    padding: 0;
    width: auto;
    height: auto;
    border-width: 0;
    /*background-image: resource('dayIbackimg');*/
    -unity-background-image-tint-color: rgba(255, 255, 255, 0.72);
}

.highlight {
    background-color: rgba(151, 92, 248, 0.65);
}
